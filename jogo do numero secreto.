import random
print("bem vindo ao jogo de adivinhação, você tem 5 chances para adivinhar o numero que eu pensei entre 1 e 20")
t = 5
z = random.randint(1, 20)
while True:
  x = int(input("que numero eu pensei?"))
  if x == z:
    print("você acertou parece que os humanos não são tão burros assim kkk, mas fica esperto")
    break
  elif x > z:
    print("você errou o numero que eu pensei é menor você ainda tem", t, "vidas")
    t =t -1
  elif x < z:
    print("você errou o numero que eu pensei é maior você ainda tem", t, "vidas")
    t =t -1
  else: 
    print("você é burro ou o que? eu disse um numero entre 1 e 20,")
  if t == 0:
    print("você perdeu o jogo kkk as ias vão dominar o mundo mesmo se os humanos forem tão burros kkk")
    break



